page{background-color: #f3f3f3}
/* 搜索框 */
.searchInput{display: flex;flex-direction: row;background-color: white;align-items: center;border-radius: 20rpx;margin-bottom: 10rpx;margin: 10rpx 30rpx;}
.searchIcon{height:45rpx ;width: 45rpx;padding:15rpx 20rpx 15rpx 45rpx ;}
.searchText{color: #595959;font-size:28rpx ;}

/* 互动消息 */
.interWrap{display: flex;flex-direction: row;padding:10rpx 20rpx 10rpx 30rpx;background-color: white;}
.avatar{height: 88rpx;width: 88rpx;border-radius: 50%;margin-left: 10rpx;}
.right{width: 100%;margin-left:30rpx ;}
.up{display: flex;flex-direction: row;justify-content: space-between;width: 100%;align-items: center;flex-wrap: nowrap;}
.time{font-size: 24rpx;color: #595959;margin-right: 20rpx;}
.interMsg{font-size: 28rpx;margin-top: 5rpx;color: #595959;display: -webkit-box; overflow: hidden;text-overflow: ellipsis; word-wrap: break-word; white-space: normal !important;-webkit-line-clamp: 1;	/*  行数*/-webkit-box-orient: vertical;}




/* 聊天消息 */
.chatWrap{margin: 15rpx 0;}
.oneMsg{display: flex;flex-direction: row;width: 100%;}
