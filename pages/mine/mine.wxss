page{background-color: #f3f3f3;height: 100%;}


.userInfo{display: flex;flex-direction: column;align-items: center;margin-bottom: 30rpx;}
.avatar{height: 120rpx;width: 120rpx;border-radius: 50%;margin: 10rpx 0;}

.datas{display: flex;flex-direction: row;justify-content: space-around;margin: 0 30rpx;background-color: white;padding: 20rpx 10rpx; border-radius: 20rpx;}
.data{font-size: 28rpx;}

.operation{display: flex;flex-direction: column;margin-top: 20rpx;}
.oper{display: flex;flex-direction: row;align-items: center;background-color: white;margin: 0rpx 30rpx 8rpx 30rpx;height: 80rpx;border-radius: 10rpx;}
.pic{width: 60rpx;margin: 0 30rpx;}


.realNameButton{background-color: white;margin-top: 15rpx;}
.realInfo{margin-left: 40rpx;margin-top: 10rpx;}

.logo{display: flex;justify-content: center;align-items: center;position: fixed;right: 0;left: 0;bottom: 0;}
.logopic{width: 500rpx;} 